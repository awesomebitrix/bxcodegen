{
    "name": "marvin255/bxcodegen",
    "description": "Code generator for bitrix",
    "type": "library",
    "keywords": ["bitrix", "code generator", "битрикс", "генератор кода"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "psr/log": "~1.0",
        "symfony/console": "~3.4",
        "symfony/yaml": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "marvin255\\bxcodegen\\": "src/",
            "marvin255\\bxcodegen\\tests\\": "tests/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/marvin255/bxcodegen"
        }
    ]
}
