{
    "name": "marvin255/bxcodegen",
    "description": "Code generator for bitrix",
    "type": "library",
    "keywords": ["bitrix", "code generator", "битрикс", "генератор кода"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "symfony/console": "~2.8 || ~3.2"
    },
    "autoload": {
        "psr-4": {
            "marvin255\\bxcodegen\\": "src/",
            "marvin255\\bxcodegen\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "friendsofphp/php-cs-fixer": "~2.2"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/marvin255/bxcodegen"
        }
    ]
}
